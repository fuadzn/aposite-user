	function get_asal_sekolah($userid, $role_id)
	{
		if ($role_id == 10) {
			$sql = "SELECT '' as id_sekolah,id_region, master_region.id_provinsi FROM master_region join dyn_user on master_region.id_region = dyn_user.id_regional where userid = ?";
			return $this->db->query($sql, array($userid));
		} else if ($role_id == 9) {
			$sql1 = "SELECT master_sekolah.id_sekolah,id_region FROM master_sekolah join dyn_user on master_sekolah.id_sekolah = dyn_user.id_sekolah where userid =?";
			return $this->db->query($sql1, array($userid));
		} else {
			$sql2 = "SELECT id_sekolah,id_regional as id_region FROM  dyn_user where userid =?";
			return $this->db->query($sql2, array($userid));
		}
	}

